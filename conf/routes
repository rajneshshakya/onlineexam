# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index()
GET   /reg                          controllers.Application.reg()
GET    /sign                        controllers.Application.login()
GET    /add                         controllers.Application.addQ()
GET     /start                      controllers.Application.start1()
GET     /submit                     controllers.Application.submit()


POST   /register                   controllers.Application.register()
POST   /login                     controllers.Application.loginStu()
POST   /addq                      controllers.Application.add()
POST    /start1                    controllers.Application.start()



# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
